              <stringProp name="script">pool_count = vars.get(&quot;pool_Id_matchNr&quot;);
script_path = vars.get(&quot;file_path&quot;);
String script_file = script_path + &quot;pools_list.csv&quot;;


// Overwrite any existing file
FileWriter fstream = new FileWriter(script_file, false);
BufferedWriter out = new BufferedWriter(fstream);

int j=0;

for (int i = 1; i &lt;= Integer.parseInt(pool_count); i++) {
	if(!((vars.get(&quot;pool_type_&quot; + i)).equals(&quot;UNMAPPED_GUEST&quot;))){
	j++;
	out.write(vars.get(&quot;pool_Id_&quot; + i));
     out.write(System.getProperty(&quot;line.separator&quot;));
     vars.put(&quot;pool_count&quot;,Integer.toString(j));
}}


out.close();
fstream.close();
// out.write(uuid_1);
</stringProp>
